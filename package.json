{
  "name": "@native-elements/funky",
  "private": false,
  "version": "0.0.2-beta.3",
  "description": "Your class-based utility",
  "main": "src/index.js",
  "files": [
    "src/index.js",
    "src/config.js",
    "src/generators"
  ],
  "scripts": {
    "test": "rimraf test/* && node src/index.js --out test/tokens.css",
    "update:deps": "yarn upgrade-interactive --latest && yarn upgrade",
    "update:cssdb": "npx browserslist@latest --update-db",
    "release": "git pull && standard-version --release-as patch && git push --follow-tags && npm publish",
    "release:minor": "git pull && standard-version --release-as minor && git push --follow-tags && npm publish",
    "release:beta": "git pull && standard-version --prerelease beta && git push --follow-tags && npm publish"
  },
  "bin": {
    "funky": "./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n-elements/funky.git"
  },
  "keywords": [],
  "author": "Mattia Astorino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/n-elements/funky/issues"
  },
  "homepage": "https://github.com/n-elements/funky#readme",
  "dependencies": {
    "@size-limit/preset-small-lib": "4.5.4",
    "clean-css": "4.2.3",
    "colorette": "1.2.1",
    "cosmiconfig": "6.0.0",
    "fs-extra": "9.0.1",
    "postcss": "7.0.32",
    "postcss-inset": "1.0.0",
    "pretty-bytes": "5.3.0",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "size-limit": "4.5.4",
    "yargs": "15.4.1"
  },
  "devDependencies": {
    "standard-version": "8.0.2"
  }
}
